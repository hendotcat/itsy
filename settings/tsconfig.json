{
  "compilerOptions": {
    "allowJs": true,
    "baseUrl": "../",
    "esModuleInterop": true,
    "jsx": "react",
    "lib": ["dom", "esnext"],
    "moduleResolution": "node",
    "paths": {
      "@highvalley.systems/itsycode/*": ["itsycode/*"],
      "@highvalley.systems/itsydraw/*": ["itsydraw/*"],
      "@highvalley.systems/itsyexpo/*": ["itsyexpo/*"],
      "@highvalley.systems/itsyhelp/*": ["itsyhelp/*"],
      "@highvalley.systems/itsyplay/*": ["itsyplay/*"],
      "@highvalley.systems/itsysite/*": ["itsysite/*"],
      "@highvalley.systems/palettes/*": ["palettes/*"],
      "@highvalley.systems/pixlflip/*": ["pixlflip/*"],
      "@highvalley.systems/settings/*": ["settings/*"],
      "@highvalley.systems/typedefs": ["typedefs"],
      "@highvalley.systems/typedefs/*": ["typedefs/*"]
    },
    "rootDirs": [
      ".",
      "../itsycode",
      "../itsydraw",
      "../itsyexpo",
      "../itsyhelp",
      "../itsyplay",
      "../itsysite",
      "../palettes",
      "../pixlflip",
      "../typedefs"
    ],
    "skipLibCheck": true
  },
  "include": ["./*"]
}
