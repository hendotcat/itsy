{
  "expo": {
    "name": "itsy studio",
    "slug": "itsy-studio",
    "privacy": "unlisted",
    "sdkVersion": "37.0.0",
    "packagerOpts": {
      "projectRoots": "",
      "config": "./metro.config.js",
      "sourceExts": ["js", "jsx", "scss", "ts", "tsx"]
    },
    "platforms": ["ios", "android"],
    "version": "1.7.0",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "systems.highvalley.itsystudio",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "UIFileSharingEnabled": true,
        "LSSupportsOpeningDocumentsInPlace": true
      },
      "supportsTablet": true,
      "usesIcloudStorage": true
    }
  }
}
